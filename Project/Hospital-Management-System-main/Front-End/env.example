# Hospital Management System - Frontend Environment Variables
# Copy this file to .env and update with your configuration

# Backend API URL
REACT_APP_API_URL=http://localhost:8080

# API Base URL for all requests
REACT_APP_BASE_URL=http://localhost:8080/api

# Application Title
REACT_APP_TITLE=Hospital Management System

# Environment (development, production)
REACT_APP_ENV=development

# Enable/Disable debug mode
REACT_APP_DEBUG=true

# Session timeout (in minutes)
REACT_APP_SESSION_TIMEOUT=30
